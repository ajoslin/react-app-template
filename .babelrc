{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "babel-plugin-react-require",
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~/": "./"
        }
      }
    ]
  ]
}
